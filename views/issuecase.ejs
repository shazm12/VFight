<body>

    <section>
        <h1 class="main-text">Issue your Case</h1>
        <form action="http://127.0.0.1:3000/issuecase" method="post">
            <h1 class="head-text">Your Name</h1> 
            <input class="input-box" type="text" name="vicname">
            <h2 class="head-text">Type of Attack</h2>
            <input  class="input-box" type="radio" name="type" value="Web Hack" />
            Web Hack
            <input  class="input-box" type="radio" name="type" value="Account Hack" />
            Account Hacking
            <input  class="input-box" type="radio" name="type" value="ARP Poisioning" />
            ARP Poisoning
            <br>
            <input class="btn btn-outline-success mt-4 input-box" type="submit" value="Submit" id="submit"/>


        </form>


    </section>


            


